<?xml version="1.0" encoding="utf-8"?>
<COLLADA xmlns="http://www.collada.org/2005/11/COLLADASchema" version="1.4.1">
  <library_geometries>
    <geometry id="Mesh" name="Mesh" />
  </library_geometries>
  <library_visual_scenes>
    <visual_scene id="Scene" name="Scene">
      <node id="Object">
        <instance_geometry url="#Mesh">
          <bind_material>
            <technique_common>
              <instance_material symbol="Material1" target="#FirstMaterial" />
              <instance_material symbol="Material2" target="#SecondMaterial" />
            </technique_common>
          </bind_material>
        </instance_geometry>
      </node>
    </visual_scene>
  </library_visual_scenes>
  <scene>
    <instance_visual_scene url="#Scene" />
  </scene>
</COLLADA>
